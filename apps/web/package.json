{
  "name": "@zagotours/web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "browserslist": [
    "last 2 versions",
    "> 0.5%",
    "not dead",
    "supports es6-module"
  ],
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
