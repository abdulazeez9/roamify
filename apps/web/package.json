    "@tanstack/react-query": "^5.90.14",
    "@zagotours/types": "workspace:*",
    "@zagotours/ui": "workspace:*",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.71.1",
    "react-icons": "^5.5.0",
