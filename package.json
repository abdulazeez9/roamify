{
  "name": "zagotours",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "test": "turbo run test"
  },
  "devDependencies": {
    "dotenv-cli": "^11.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "turbo": "^2.7.1",
    "typescript": "5.9.3",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "js-yaml": "^4.1.1"
    },
    "ignoredBuiltDependencies": [
      "core-js",
      "esbuild",
      "msw"
    ],
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "bcrypt",
      "prisma",
      "sharp"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "vitest run"
    ]
  }
}
